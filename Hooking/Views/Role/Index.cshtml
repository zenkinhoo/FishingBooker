@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
@{
    ViewData["Title"] = "Uloge";
}

<h1>Uloge</h1>
<a asp-action="Create">Kreiraj novu ulogu</a><br/>
<table id="not-smartTable" class="table table-striped table-bordered">
<thead>
<tr>
    <td>Name</td>
</tr>
</thead>
<tbody>
@foreach (var role in Model)
{
    <tr>
        <td hidden>@role.Id</td>
        <td>@role.Name</td>
    </tr>

}
</tbody>
</table>