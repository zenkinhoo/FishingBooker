@model Hooking.Models.InstructorNotAvailablePeriod

@{
    ViewData["Title"] = "Create";
}

<h1>Unesite kriterijum za pretragu instruktora</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-controller="Adventures" asp-action="InstructorFiltered">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="StartTime" class="control-label"></label>
                <input asp-for="StartTime" class="form-control" />
                <span asp-validation-for="StartTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EndTime" class="control-label"></label>
                <input asp-for="EndTime" class="form-control" />
                <span asp-validation-for="EndTime" class="text-danger"></span>
                <div class="form-group">
                    <input type="submit" value="Pretrazi" class="btn btn-primary" />
                </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
